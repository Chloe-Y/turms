    "lodash": "^4.17.21",
    "markdown-it": "^12.1.0",
    "moment": "^2.29.1",
    "save-svg-as-png": "^1.4.17",
    "turms-client-js": "file:../turms-client-js",
    "vue": "^3.1.5",
    "vue-i18n": "^9.1.7",
    "vue-router": "^4.0.10",
