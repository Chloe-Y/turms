    }
  ],
  "bugs": {
    "url": "https://github.com/turms-im/turms/issues"
  },
  "homepage": "https://github.com/turms-im/turms#readme",
  "dependencies": {
    "protobufjs": "^6.11.2",
    "unfetch": "^4.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.17.4",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
